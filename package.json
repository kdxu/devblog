{
  "name": "gatsby-starter-material",
  "description": "GatsbyJS Material Blog theme.",
  "version": "1.1.0",
  "author": "Ruben Harutyunyan <vagr9k@gmail.com>",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/",
    "lint:ja": "textlint content/",
    "write-good": "write-good $(glob 'content/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "gatsby": "2.32.3",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-catch-links": "2.10.0",
    "gatsby-plugin-feed": "2.13.0",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-lodash": "3.10.0",
    "gatsby-plugin-manifest": "2.12.0",
    "gatsby-plugin-netlify-cms": "4.10.0",
    "gatsby-plugin-nprogress": "2.10.0",
    "gatsby-plugin-offline": "3.10.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sass": "3.2.0",
    "gatsby-plugin-sharp": "2.14.1",
    "gatsby-plugin-sitemap": "2.12.0",
    "gatsby-plugin-twitter": "2.10.0",
    "gatsby-remark-autolink-headers": "2.11.0",
    "gatsby-remark-copy-linked-files": "2.10.0",
    "gatsby-remark-images": "3.11.0",
    "gatsby-remark-prismjs": "3.13.0",
    "gatsby-remark-relative-images": "2.0.2",
    "gatsby-remark-responsive-iframe": "2.11.0",
    "gatsby-source-filesystem": "2.11.0",
    "gatsby-transformer-remark": "2.16.0",
    "gatsby-transformer-sharp": "2.12.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "netlify-cms": "2.10.90",
    "node-sass": "^5.0.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-md": "^2.5.5",
    "react-share": "^4.3.1",
    "react-twitter-widgets": "^1.9.5",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "^2.2.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "textlint": "^11.8.2",
    "textlint-rule-max-ten": "^2.0.4",
    "textlint-rule-no-mix-dearu-desumasu": "^4.0.1",
    "textlint-rule-preset-jtf-style": "^2.3.6",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "write-good": "^1.0.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
